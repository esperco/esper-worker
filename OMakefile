OCAML_LIBS = $(OCAML_LIBS_worker)

FILES[] =
  worker_jobid
  worker_t
  worker_j
  worker_v
  worker_access
  worker
  worker_main

Atdgen(worker, -j-std)
OCamlLibrary(worker, $(FILES))
Meta(worker)

.DEFAULT: META worker.cma worker.cmxa

.PHONY: clean
clean:
  rm -f *.o *.a *.cm* *~ *.annot *.run *.opt META $(ATDGEN_OUTFILES)
